{
  "name": "BPM Tapper",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Find the tempo of your music by clicking in time with the beat.",
  "homepage_url": "https://github.com/chrisblazek/bpm-tapper",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["src/event/event.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "BPM Tapper",
    "default_popup": "src/popup/popup.html"
  }
}